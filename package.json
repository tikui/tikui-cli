{
  "name": "@tikui/cli",
  "version": "2.4.3",
  "description": "Command lines tool for Tikui",
  "main": "index.js",
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && cp -r src/cli/generate-project dist/cli/generate-project",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "prepare": "husky"
  },
  "repository": "github:tikui/tikui-cli",
  "keywords": [
    "CLI"
  ],
  "author": {
    "name": "Anthony Rey"
  },
  "contributors": [
    {
      "name": "Arnaud Freismuth"
    },
    {
      "name": "Jérémie Tisserand"
    },
    {
      "name": "Hippolyte Durix"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tikui/tikui-cli/issues"
  },
  "homepage": "https://github.com/tikui/tikui-cli#readme",
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "module-alias": "^2.2.2",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/jest": "^29.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-jest": "^28.8.0",
    "husky": "^9.1.2",
    "jest": "^29.2.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.0.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "bin": {
    "tikuicli": "./dist/bin/tikuicli.js",
    "tikui": "./dist/bin/tikuicli.js"
  }
}
